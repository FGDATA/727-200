  <fx>
    <engine>
      <name>left run</name>
      <path>Aircraft/727-200/Sounds/eng/idling.wav</path>
      <mode>looped</mode>
      <condition>
        <and>
          <greater-than>
            <property>/engines/engine[0]/rpm</property>
            <value>0.1</value>
          </greater-than>
	  <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <pitch>
        <property>/engines/engine[0]/n1</property>
	<factor>0.018</factor>
	<offset>0</offset>
        <min>0.0</min>
        <max>3.5</max>
      </pitch>
      <volume>
        <property>/engines/engine[0]/n1</property>
	<factor>0.02</factor>
	<offset>0</offset>
        <min>0.0</min>
        <max>1.5</max>
      </volume>
      <position>
        <x>0</x>
        <y>9.8</y>
        <z>-3</z>
      </position>
      <reference-dist>1.0</reference-dist>
      <max-dist>2.0</max-dist>
    </engine>
    
