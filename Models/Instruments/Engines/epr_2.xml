<?xml version="1.0"?>

<PropertyList>

<path>instr_epr.ac</path>

	<animation>
		<name>epr2</name>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		<object-name>shadow</object-name>
		<object-name>Bezel_35</object-name>
		<object-name>Knob</object-name>
		<object-name>epr-limit</object-name>
		<object-name>epr1d</object-name>
		<object-name>epr2d</object-name>
		<object-name>epr3d</object-name>
		<object-name>eprdot</object-name>
	</animation>

	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		  <emission>
<factor-prop>controls/lighting/panel-light</factor-prop>
      <red>0.5</red>
      <green>0.5</green>
      <blue>0.5</blue>
  </emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		<object-name>shadow</object-name>
		<object-name>epr-limit</object-name>
		<object-name>epr1d</object-name>
		<object-name>epr2d</object-name>
		<object-name>epr3d</object-name>
		<object-name>eprdot</object-name>
		<condition>
			<not><property>sim/rendering/rembrandt/enabled</property></not>
			<property>systems/electrical/outputs/cabin-dim</property>
		</condition>
		<emission>
		  <red>0.8</red>
		  <green>0.8</green>
		  <blue>0.8</blue>
                     <factor-prop>controls/lighting/panel-light</factor-prop>
		</emission>
	</animation>	

	<animation>
		<type>noshadow</type>
		<object-name>shadow</object-name>
		<condition>
			<not><property>sim/rendering/rembrandt/enabled</property></not>
			<property>systems/electrical/outputs/cabin-dim</property>
		</condition>
	</animation>

	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>shadow</object-name>
	</effect>
	
	<animation>
		<type>textranslate</type>
		<object-name>epr1d</object-name>
		<property>controls/engines/engine[1]/epr-lim</property>
		<bias>0.001</bias>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>epr2d</object-name>
		<property>controls/engines/engine[1]/epr-lim</property>
		<bias>0.001</bias>
		<factor>1</factor>
		<step>0.1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>epr3d</object-name>
		<property>controls/engines/engine[1]/epr-lim</property>
		<bias>0.001</bias>
		<factor>10</factor>
		<step>0.01</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	
<animation>
	<type>pick</type>
	<object-name>Knob</object-name>
	<action>
		<button>0</button>
		<button>3</button>
		<binding>
			<command>property-adjust</command>
			<property>controls/engines/engine[1]/epr-lim</property>
			<min>1.0</min>
			<max>3.0</max>
			<step>0.01</step>
			<wrap>false</wrap>
		</binding>
	</action>
	<action>
		<button>1</button>
		<button>4</button>
		<binding>
			<command>property-adjust</command>
			<property>controls/engines/engine[1]/epr-lim</property>
			<min>1.0</min>
			<max>3.0</max>
			<step>-0.01</step>
			<wrap>false</wrap>
		</binding>
	</action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>engines/engine[1]/epr-actual</property>
  <interpolation>
    <entry><ind>1.0</ind><dep>  5</dep></entry>
    <entry><ind>1.4</ind><dep> 73</dep></entry>
    <entry><ind>1.8</ind><dep>139</dep></entry>
    <entry><ind>2.2</ind><dep>205</dep></entry>
    <entry><ind>2.6</ind><dep>269</dep></entry>
    <entry><ind>3.0</ind><dep>335</dep></entry>
  </interpolation>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>epr-limit</object-name>
  <property>controls/engines/engine[1]/epr-lim</property>
  <interpolation>
    <entry><ind>1.0</ind><dep>  5</dep></entry>
    <entry><ind>1.4</ind><dep> 73</dep></entry>
    <entry><ind>1.8</ind><dep>139</dep></entry>
    <entry><ind>2.2</ind><dep>205</dep></entry>
    <entry><ind>2.6</ind><dep>269</dep></entry>
    <entry><ind>3.0</ind><dep>335</dep></entry>
  </interpolation>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>
